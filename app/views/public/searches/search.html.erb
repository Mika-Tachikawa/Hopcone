<% if @model == 'user' %>
	<h3>Users search for "<%= @content %>"</h3>
  <%= render 'public/accounts/index', account: @records%>
<% elsif @model == 'review' %>
	<h3>Reviews search for "<%= @content %>"</h3>
  <%= render 'public/reviews/index', reviews: @records %>
<% elsif @model == 'tag' %>
	<h3>Tags search for "<%= @content %>"</h3>
  <%= render 'public/tags/index', tags: @records %>
<% end %>